    "lodash": "^4.17.15",
    "popper.js": "^1.12",
    "vue": "^2.5.7"
  },
  "dependencies": {
    "laravel-echo": "^1.3.5",
    "moment": "^2.24.0",
    "pusher-js": "^4.2.2",
    "vue-circular-count-down-timer": "^1.0.4",
    "vue-router": "^3.0.7",
    "vue-simplemde": "^0.4.6",
    "vuejs-countdown-timer": "^2.1.1",
    "vuetify": "^1.0.4",
